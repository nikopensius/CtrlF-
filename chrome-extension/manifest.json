{
    "manifest_version": 2,
    "name": "CtrlF+",
    "version": "1.0",
    "description": "Find bar with advanced functionality",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html",
        "default_title": "CtrlF+"
    },
    "permissions": [
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    }
}